{
  "framework": "vite",
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/proxy/:path*",
      "destination": "/api/proxy?path=:path*"
    },
    {
      "source": "/api/melolo/:path*",
      "destination": "/api/melolo?path=:path*"
    },
    {
      "source": "/api/netshort/:path*",
      "destination": "/api/netshort?path=:path*"
    },
    {
      "source": "/api/reelife/:path*",
      "destination": "/api/reelife?path=:path*"
    },
    {
      "source": "/((?!api/).*)",
      "destination": "/index.html"
    }
  ]
}
